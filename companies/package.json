{
  "name": "cspell-dict-companies",
  "version": "1.0.0",
  "description": "Companies dictionary for cspell.",
  "bin": {
    "cspell-dict-companies-link": "./link.js",
    "cspell-dict-companies-unlink": "./unlink.js"
  },
  "scripts": {
    "build": "cspell-tools compile \"companies.txt\" -o .",
    "cspell-link": "node link.js",
    "cspell-unlink": "node unlink.js",
    "publish-minor": "npm version minor && npm publish",
    "publish-patch": "npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jason3S/cspell-dicts.git"
  },
  "keywords": [
    "cspell",
    "companies",
    "companies",
    "dictionary",
    "spelling"
  ],
  "author": "Jason Dent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jason3S/cspell-dicts/issues"
  },
  "homepage": "https://github.com/Jason3S/cspell-dicts/blob/master/companies/README.md",
  "devDependencies": {
    "cspell-tools": "^1.4.0"
  },
  "dependencies": {
    "configstore": "^3.1.0"
  },
  "files": [
    "companies.txt.gz",
    "cspell-ext.json",
    "*.js"
  ]
}
